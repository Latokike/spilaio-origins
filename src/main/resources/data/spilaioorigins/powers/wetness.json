{
  "type": "origins:multiple",
  "bar": {
    "type": "origins:resource",
    "min": 0,
    "max": 1,
    "hud_render": {
      "should_render": false
    }
  },
  "increment": {
  	"type": "origins:action_over_time",
  	"entity_action": {
      "type": "origins:change_resource",
      "resource": "spilaioorigins:wetness.bar",
      "change": 0.01,
  	},
  	"interval": 1,
  	"condition": {
    	"type": "origins:fluid_height",
		  "fluid": "minecraft:water",
		  "comparison": ">",
		  "compare_to": 0.0
    }
  },
  "decrement": {
  	"type": "origins:action_over_time",
  	"entity_action": {
      "type": "origins:change_resource",
      "resource": "spilaioorigins:wetness.bar",
      "change": -0.005,
  	},
  	"interval": 1,
  	"condition": {
    	"type": "origins:fluid_height",
		  "fluid": "minecraft:water",
		  "comparison": "<=",
		  "compare_to": 0.0
    }
}
